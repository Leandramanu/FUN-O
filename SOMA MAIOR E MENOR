#include<stdio.h>
int MaiorMenor(int a, int b, int c)
{
    int maior, menor;
    if(a >= b && a >=c)
    {
        maior = a;
    }
    else if(b >= a && b>=c)
    {
        maior = b;
    }
    else
    {
        maior = c;
    }
    if(a <= b && a <=c)
    {
        menor = a;
    }
    else if(b <= a && b <=c)
    {
        menor = b;
    }
    else
    {
        menor = c;
    }

    int soma = maior + menor;

}
int main(){

    int a, b, c;
    scanf("%d", &a);
    scanf("%d", &b);
    scanf("%d", &c);

    int soma = MaiorMenor(a,b,c);
    printf("%d\n", soma);

    return 0;
}
